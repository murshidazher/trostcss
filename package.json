{
  "name": "trostcss",
  "version": "1.0.0",
  "description": "Modern CSS framework",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murshidazher/trostcss.git"
  },
  "keywords": [
    "trost",
    "trostcss",
    "css",
    "framework"
  ],
  "author": "Murshid Azher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/murshidazher/trostcss/issues"
  },
  "homepage": "https://github.com/murshidazher/trostcss#readme",
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
